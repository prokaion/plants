<p id="notice"><%= notice %></p>

<h1>Listing Paths</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Shop</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_paths.each do |admin_path| %>
      <tr>
        <td><%= admin_path.name %></td>
        <td><%= admin_path.shop.name %></td>
        <td><%= link_to 'Show', admin_path %></td>
        <td><%= link_to 'Edit', edit_admin_path_path(admin_path) %></td>
        <td><%= link_to 'Destroy', admin_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Path', new_admin_path_path %>
