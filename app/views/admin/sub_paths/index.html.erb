<p id="notice"><%= notice %></p>

<h1>Listing Sub Paths</h1>

<table>
  <thead>
    <tr>
      <th>Part</th>
      <th>Path</th>
      <th>Terminator</th>
      <th>Id ref</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_sub_paths.each do |admin_sub_path| %>
      <tr>
        <td><%= admin_sub_path.part %></td>
        <td><%= admin_sub_path.path_id %></td>
        <td><%= admin_sub_path.terminator %></td>
        <% if admin_sub_path.id_ref %>
          <td><%= admin_sub_path.id_ref %></td>
        <% else %>
          <td>None</td>
        <% end %>
        <td><%= link_to 'Show', admin_sub_path %></td>
        <td><%= link_to 'Edit', edit_admin_sub_path_path(admin_sub_path) %></td>
        <td><%= link_to 'Destroy', admin_sub_path, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sub path', new_admin_sub_path_path %>
